<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <servlet>
        <servlet-name>CookieTest</servlet-name>
        <servlet-class>CookieTest</servlet-class>
    </servlet>
	    <servlet>
        <servlet-name>CheckCookie</servlet-name>
        <servlet-class>CheckCookie</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CookieTest</servlet-name>
        <url-pattern>/CookieTest.do</url-pattern>
    </servlet-mapping>
	    <servlet-mapping>
        <servlet-name>CheckCookie</servlet-name>
        <url-pattern>/checkcookie.do</url-pattern>
    </servlet-mapping>
</web-app>
