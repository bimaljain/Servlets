<param-name>adminEmail1</param-name>
<param-value>likewecare@wickedlysmart.com</param-value>
</init-param>
</servlet>

<context-param>
<param-name>adminEmail2</param-name>
<param-value>clientheaderror@wickedlysmart.com</param-value>
</context-param>

<servlet-mapping>
<servlet-name>TestInit1</servlet-name>
<url-pattern>/TestInit1.do</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>TestInit2</servlet-name>
<url-pattern>/TestInit2.do</url-pattern>
